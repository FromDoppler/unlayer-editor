{
  "name": "unlayer-editor",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^8.7.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.20",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.14",
    "@types/react-email-editor": "^1.1.2",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "browserslist": "^4.17.2",
    "camelcase": "^6.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^5.2.7",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eclint": "^2.8.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "eslint-webpack-plugin": "^2.5.4",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "^4.5.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-resolve": "^26.6.2",
    "jest-watch-typeahead": "^0.6.5",
    "mini-css-extract-plugin": "^1.6.2",
    "npm-watch": "^0.11.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "pnp-webpack-plugin": "^1.7.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.4.1",
    "prompts": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.13.1",
    "react-email-editor": "^1.3.0",
    "react-refresh": "^0.9.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^10.2.0",
    "semver": "^7.3.5",
    "style-loader": "^2.0.0",
    "styled-components": "^4.4.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-pnp": "^1.2.0",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "web-vitals": "^2.1.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "watch": {
    "dev-build": {
      "patterns": [
        "src/customJs"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm-watch\" \"node scripts/start.js\"",
    "dev-build": "webpack --config webpack.config-dev.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "coverage:local": "node scripts/test.js --coverage --watchAll",
    "coverage:ci": "node scripts/test.js --coverage",
    "verify-format": "yarn eslint-check && yarn prettier-check && yarn eclint-check",
    "eclint-fix": "eclint fix",
    "eclint-check": "eclint check",
    "eslint-check": "eslint --ext .ts --ext .tsx .",
    "prettier": "prettier --write \"**/**\"",
    "prettier-check": "prettier --check \"**/**\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,tsx,ts}",
      "!**/src/customJs/**",
      "!**/.github/**",
      "!**/.vscode/**",
      "!**/cdn-hepers/**",
      "!**/coverage/**",
      "!**/docs/**",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/setupTests.ts",
      "!**/reportWebVitals.ts",
      "!**/react-app-env.d.ts",
      "!**/App.tsx"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
